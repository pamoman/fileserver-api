{
  "kind": "collectionType",
  "collectionName": "documents",
  "info": {
    "name": "Document"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "expires": {
      "type": "date"
    },
    "fil": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files"
      ],
      "plugin": "upload",
      "required": true
    },
    "subject": {
      "model": "subject",
      "via": "documents"
    }
  }
}
